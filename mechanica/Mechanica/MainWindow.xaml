<Window x:Class="Mechanica.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mechanica"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="input_distance_txt" HorizontalAlignment="Left" Height="23" Margin="33,72,0,0" TextWrapping="Wrap" Text="Distance" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="input_distance" HorizontalAlignment="Left" Margin="33,59,0,0" TextWrapping="Wrap" Text="Desired Distance:" VerticalAlignment="Top"/>
        <Button x:Name="begin_test_btn" Content="Test Now" HorizontalAlignment="Left" Margin="158,72,0,0" VerticalAlignment="Top" Width="75" Click="begin_test_btn_Click"/>
        <TextBlock x:Name="lvdt_data_rd" HorizontalAlignment="Left" Margin="270,68,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="105"/>
        <TextBlock x:Name="loadcell_data_rd" HorizontalAlignment="Left" Margin="396,65,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="99"/>
        <TextBlock HorizontalAlignment="Left" Margin="272,50,0,0" TextWrapping="Wrap" Text="LVDT Data:" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="397,42,0,0" TextWrapping="Wrap" Text="Load Cell Data:" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
